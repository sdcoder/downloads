            <add input="{REQUEST_URI}" pattern="/AppStatus/_fix_for_ie_crossdomain__" />
            <add input="{REQUEST_URI}" pattern="/AppStatus/PreFunding/_fix_for_ie_crossdomain__" />
            <add input="{REQUEST_FILENAME}" pattern="_fix_for_ie_crossdomain__" />
          </conditions>
          <action type="CustomResponse" statusCode="404" />
        </rule>
        <rule name="GonePages">
          <match url="^(.*)$" />
          <conditions logicalGrouping="MatchAny">
            <add input="{REQUEST_URI}" pattern="/Assets/Images/low_rate_promise.gif" />
            <add input="{REQUEST_URI}" pattern="/aboutus/Investors.aspx" />
            <add input="{REQUEST_URI}" pattern="/AppStatus/OldRedirect" />
            <add input="{REQUEST_URI}" pattern="/Misc/ViewEdoc.aspx" />
            <add input="{REQUEST_URI}" pattern="^/PreFunding/" />
            <add input="{REQUEST_URI}" pattern="/wp-login.php" />

            <!--malicious requests-->
            <add input="{REQUEST_URI}" pattern="/CMSPages/Logon.aspx" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="/CMSPages/PortalTemplate.aspx" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="^.*\.(ini|config|jsp)$" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="^.*require\.js$" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="^.*XSQLConfig\.xml$" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="^.*EventHub\.js" ignoreCase="true" />
          </conditions>
          <action type="CustomResponse" statusCode="410" statusReason="Gone" statusDescription="The requested resource is no longer available" />
        </rule>

