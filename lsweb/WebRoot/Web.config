  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
  <system.web>
    <!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
        -->
    <compilation debug="true" targetFramework="4.5.1" />
    <!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
  
  -->
    <membership defaultProvider="FirstAgainMembershipProvider">
      <providers>
        <add name="FirstAgainMembershipProvider" type="FirstAgain.Domain.ServiceModel.Client.FirstAgainMembershipProvider" />
      </providers>
    </membership>
    <!--
            The <customErrors> section enables configuration 
            of what to do if/when an unhandled error occurs 
            during the execution of a request. Specifically, 
            it enables developers to configure html error pages 
            to be displayed in place of a error stack trace.
    
    FirstAgain Note:  Our errors are handled before this in Global.asax. 
                      Because of that, implementing this has no affect.
    -->
    <pages enableViewState="false" enableEventValidation="false" viewStateEncryptionMode="Never" controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" maxPageStateFieldLength="32767">
    </pages>
    <authentication mode="Forms">
      <!-- Set the timeout to be one minute less than the server's session timeout value in order to prevent null reference violations to an expired session object 
      (using 12 now - to handle the one and a half length issue, since server session timeout is 20) -->
      <forms loginUrl="~/customer-sign-in" protection="All" timeout="12" path="/" requireSSL="false" slidingExpiration="true" cookieless="UseCookies" />
    </authentication>

    <httpModules>
      <add name="URLRewriteModule" type="LightStreamWeb.URLRewriteModule, LightStreamWeb" />
      <add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
    </httpModules>
    <!-- This is mainly for file upload but is probably a good idea to apply site-wide.  This could be made location-specific 
      if machine.config is modified to allow location overrides for httpRuntime.
    -->    
    <!--
            Opt into the 4.5.1 functionality.  Adding this
            ensures that our application does not use quirks
            mode.
        -->
    <httpRuntime maxRequestLength="12228" executionTimeout="300" enableVersionHeader="false" targetFramework="4.5.1" />
    <httpHandlers>
      <add path="*" verb="OPTIONS, PROPFIND" type="System.Web.DefaultHttpHandler" validate="true" />
    </httpHandlers>
</system.web>
  
