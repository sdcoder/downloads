@model string

<div class="experienceImage">
    <iframe id="videoWhyWeAreDiff@(Guid.NewGuid().ToString("N"))"
            title="What is the LightStream loan?"
            style="height: 100%; width: 100%; padding: 0; margin: 0"
            width="640" 
            height="360"
            gesture="media"
            allow="encrypted-media"
            tabindex="0"
            frameborder="0"
            webkitallowfullscreen
            mozallowfullscreen
            allowfullscreen
            src="@(String.Format("{0}?controls=1&rel=0&showinfo=0&autoplay=0&modestbranding=1&enablejsapi=1&html5=1&fs=0", Model))"></iframe>
</div>
<h3 class="thatBlueFontTho characteristicTitle" tabindex="0">Why we're different</h3>
<p class="characteristicDescription" tabindex="0">
    With LightStream you know you'll get a great rate, and the best loan process you've ever experienced – guaranteed.
</p>